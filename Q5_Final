class Main {
  public static void main(String[] args) {
    int i,j;
    int z = 0;
    for (i =1, i<=n; i++){
    	int count = 0;
    	for(int j; j<=n;i++){
	    	if( x[i][j] == 0)
		    	count++;
	    }
    	if(count==n && z=0){
	    	system.out.println(“First all-zero row is:” + i.toString());
	    	i++;
    	}
    }
  }
}
